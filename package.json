{
  "name": "manifest-root",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:docs": "turbo run build --filter=@project44-manifest/docs",
    "build:packages": "turbo run build --filter='./packages/*'",
    "changeset": "changeset",
    "clean": "turbo run clean",
    "dev": "turbo run dev --filter=@project44-manifest/storybook",
    "dev:docs": "turbo run dev --filter=@project44-manifest/docs",
    "format": "prettier . --write",
    "lint": "eslint --cache --color --fix --ext .ts,.tsx ./packages",
    "prepare": "husky install",
    "release": "yarn changeset publish",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "turbo run typecheck",
    "version": "yarn changeset version && yarn install --mode=update-lockfile"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.9",
    "babel-preset-moon": "^1.0.2",
    "eslint": "^8.24.0",
    "eslint-config-moon": "^1.0.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "jest-preset-moon": "^1.0.2",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "packemon": "^2.4.1",
    "prettier": "^2.7.1",
    "prettier-config-moon": "^1.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-moon": "^1.0.1",
    "turbo": "^1.5.4",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
  },
  "packageManager": "yarn@3.2.0",
  "engines": {
    "node": "16.16.0"
  }
}
