    "@percy/storybook": "^4.3.4",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@swc/core": "^1.3.25",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
