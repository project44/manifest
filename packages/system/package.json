{
  "name": "@project44-manifest/system",
  "version": "0.1.5",
  "description": "Manifest Design System core utilities",
  "license": "MIT",
  "author": "project44",
  "keywords": [
    "manifest",
    "design",
    "system",
    "react"
  ],
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "packages/system"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "packemon build --addEngines --declaration --declarationConfig tsconfig.build.json",
    "clean": "rimraf .turbo && packemon clean",
    "dev": "packemon watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@project44-manifest/react-test-utils": "*",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tsconfig-manifest": "*"
  },
  "packemon": {
    "platform": "browser"
  }
}
