{
  "name": "@project44-manifest/react-icons",
  "version": "1.4.0",
  "description": "Manifest Design System react icon library",
  "license": "MIT",
  "author": "project44",
  "keywords": [
    "manifest",
    "design",
    "system",
    "react",
    "icons"
  ],
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "packemon build --addEngines --declaration --declarationConfig tsconfig.build.json",
    "clean": "packemon clean",
    "generate-icons": "node ./scripts/generate-icons.mjs",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@project44-manifest/react-icon": "^0.2.3",
    "@project44-manifest/react-styles": "^2.0.0"
  },
  "devDependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "packemon": {
    "platform": "browser"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    }
  }
}
