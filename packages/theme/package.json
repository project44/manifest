{
  "name": "@project44-manifest/theme",
  "version": "0.1.0",
  "description": "Manifest Design System themes",
  "license": "MIT",
  "author": "project44",
  "keywords": [
    "manifest",
    "design",
    "system"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "packages/theme"
  },
  "scripts": {
    "build": "run-s build:types build:js",
    "build:js": "rollup -c",
    "build:types": "tsc -b tsconfig.build.json",
    "clean": "rm -rf .turbo node_modules lib esm dts *.tsbuildinfo",
    "type-check": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@project44-manifest/design-tokens": "^1.1.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7"
  }
}
