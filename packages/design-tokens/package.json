{
  "name": "@project44-manifest/design-tokens",
  "version": "1.1.0",
  "description": "Manifest Design System design tokens",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.js",
      "require": "./dist/main.js"
    },
    "./json/*": "./dist/json/*",
    "./css/*": "./dist/css/*"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "packages/design-tokens"
  },
  "scripts": {
    "build": "run-s build:tokens build:package",
    "build:tokens": "node scripts/build.js",
    "build:package": "parcel build",
    "clean": "rimraf .turbo dist",
    "dev": "chokidar \"tokens/**/*.json\" -c \"yarn build\"",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "style-dictionary": "^3.7.0"
  }
}
