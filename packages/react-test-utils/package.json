{
  "name": "@project44-manifest/react-test-utils",
  "version": "0.2.1",
  "description": "Manifest Design System react test utilities",
  "license": "MIT",
  "author": "project44",
  "keywords": [
    "manifest",
    "design",
    "system",
    "react",
    "jest"
  ],
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "packages/react-test-utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "packemon build --addEngines --declaration --declarationConfig tsconfig.build.json",
    "clean": "rimraf .turbo && packemon clean",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@react-aria/overlays": "^3.10.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest-axe": "^3.5.5",
    "jest-axe": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-manifest": "*",
    "tsconfig-manifest": "*"
  },
  "packemon": {
    "platform": "browser"
  }
}
