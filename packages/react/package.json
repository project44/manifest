    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
