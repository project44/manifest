{
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "emitDeclarationOnly": false,
    "incremental": true,
    "jsx": "preserve",
    "noEmit": true,
    "paths": {
      "contentlayer/generated": [
        "./.contentlayer/generated"
      ]
    },
    "strict": false
  },
  "include": [
    "src",
    "**/*.json",
    "next-env.d.ts",
    ".contentlayer/generated"
  ],
  "exclude": [
    "node_modules"
  ]
}
