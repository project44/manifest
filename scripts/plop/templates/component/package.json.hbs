{
  "name": "@project44-manifest/react-{{dashCase componentName}}",
  "version": "0.1.0",
  "description": "{{description}}",
  "license": "MIT",
  "author": "project44",
  "keywords": [
    "manifest",
    "design",
    "system",
    "react",
    "components"
  ],
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dts/index.d.ts",
      "browser": {
        "module": "./esm/index.js",
        "import": "./esm/index.js",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git"
  },
  "scripts": {
    "build": "packemon build --addEngines --addExports --declaration --declarationConfig tsconfig.build.json",
    "clean": "packemon clean && rm -rf .turbo node_modules *.tsbuildinfo",
    "typecheck": "tsc -b"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@project44-manifest/react-styles": "^0.2.0",
    "@project44-manifest/react-types": "^0.1.0"
  },
  "devDependencies": {
    "@project44-manifest/test-utils": "^0.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "packemon": {
    "platform": "browser"
  }
}
