{
  "name": "jest-preset-manifest",
  "version": "1.0.0",
  "description": "Manifest Design System jest preset",
  "license": "MIT",
  "author": "project44",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "jest-preset.js",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:project-44/manifest.git",
    "directory": "tools/jest-preset"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "jest": "^29.3.1"
  },
  "dependencies": {
    "jest-environment-jsdom": "^29.3.1"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "fileMock": "src/fileMock.ts"
    },
    "format": "lib",
    "platform": "node"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "exports": {
    "./package.json": "./package.json",
    "./jest-preset": "./lib/index.js",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": "./lib/*.js",
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    }
  }
}
